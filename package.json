{
  "name": "form-generate-page",
  "version": "1.0.0",
  "description": "Form generator application with React frontend and Hono backend",
  "type": "module",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "npm run dev --workspaces",
    "build": "npm run build --workspaces",
    "start": "npm run start --workspaces",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "install-all": "npm install"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}